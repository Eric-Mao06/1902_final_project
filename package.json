{
  "name": "linkd",
  "version": "1.0.0",
  "scripts": {
    "start": "cd backend && uvicorn src.app:app --host 0.0.0.0 --port $PORT",
    "start-frontend": "cd frontend && npm run dev",
    "start-backend": "cd backend && python run.py",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  }
}
